{
  "ts-node": {
    "transpileOnly": true
  },
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "outDir": "../out-tsc/dev-scripts",
    "allowSyntheticDefaultImports": true,
    "importHelpers": true,
    "strict": true,
    "alwaysStrict": true,
    "strictNullChecks": false,
    "esModuleInterop": true,
    "target": "ES2020",
    "types": ["node"]
  }
}
