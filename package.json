{
  "name": "fantasticon",
  "version": "0.0.0",
  "description": "Icon font generation tool",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tancredi/fantasticon.git"
  },
  "keywords": [
    "icons",
    "fonts",
    "iconfonts",
    "svg",
    "vector"
  ],
  "author": "Tancredi Trugenberger",
  "bugs": {
    "url": "https://github.com/tancredi/fantasticon/issues"
  },
  "homepage": "https://github.com/tancredi/fantasticon#readme",
  "main": "lib/index.js",
  "bin": {
    "fantasticon": "bin/fantasticon"
  },
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "build": "rimraf lib && tsc --project ./tsconfig.json",
    "watch": "npm run build -- --watch",
    "lint": "prettier --check .",
    "semantic-release": "semantic-release"
  },
  "files": [
    "bin",
    "lib",
    "templates"
  ],
  "prepublish": "build",
  "dependencies": {
    "change-case": "^4.1.2",
    "cli-color": "^2.0.0",
    "commander": "^6.2.1",
    "glob": "^7.1.6",
    "handlebars": "^4.7.6",
    "lodash": "^4.17.20",
    "slugify": "^1.4.6",
    "svg2ttf": "^5.0.0",
    "svgicons2svgfont": "^9.1.1",
    "ttf2eot": "^2.0.0",
    "ttf2woff": "^2.0.2",
    "ttf2woff2": "^4.0.1"
  },
  "devDependencies": {
    "@types/cli-color": "^2.0.0",
    "@types/glob": "^7.1.3",
    "@types/lodash": "^4.14.166",
    "@types/node": "^14.14.16",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "semantic-release": "^17.3.0",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "rootDir": "./src"
  },
  "engines": {
    "node": ">= 10.0.0"
  },
  "files": [
    "bin/fantasticon",
    "lib/**/*.{js,ts,map}",
    "!lib/**/{__mocks__,__tests__}/",
    "templates/*.hbs"
  ]
}
