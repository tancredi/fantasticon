{
  "name": "fantasticon",
  "version": "0.0.0",
  "description": "Icon font generation tool",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tancredi/fantasticon.git"
  },
  "keywords": [
    "icons",
    "fonts",
    "iconfonts",
    "svg",
    "vector"
  ],
  "author": "Tancredi Trugenberger",
  "bugs": {
    "url": "https://github.com/tancredi/fantasticon/issues"
  },
  "homepage": "https://github.com/tancredi/fantasticon#readme",
  "main": "lib/index.js",
  "bin": {
    "fantasticon": "bin/fantasticon"
  },
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "build": "rimraf lib && tsc --project src/tsconfig.json",
    "watch": "npm run build -- --watch",
    "typecheck": "tsc --noEmit --project src/tsconfig.test.json",
    "lint": "eslint '**/*.{ts,js}'"
  },
  "files": [
    "bin/fantasticon",
    "lib/**/*.{js,ts,map}",
    "!lib/**/{__mocks__,__tests__}/",
    "templates/*.hbs"
  ],
  "prepublish": "build",
  "dependencies": {
    "change-case": "^4.1.2",
    "cli-color": "^2.0.2",
    "commander": "^9.3.0",
    "glob": "^8.0.3",
    "handlebars": "^4.7.7",
    "slugify": "^1.6.5",
    "svg2ttf": "^6.0.3",
    "svgicons2svgfont": "^12.0.0",
    "ttf2eot": "^3.1.0",
    "ttf2woff": "^3.0.0",
    "ttf2woff2": "^4.0.5"
  },
  "devDependencies": {
    "@types/cli-color": "^2.0.2",
    "@types/eslint": "^8.4.3",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/glob": "^7.2.0",
    "@types/jest": "^28.1.2",
    "@types/node": "^16.10.2",
    "@types/prettier": "^2.6.3",
    "@types/rimraf": "^3.0.2",
    "@types/semantic-release": "^17.2.3",
    "@types/svg2ttf": "^5.0.1",
    "@types/svgicons2svgfont": "^10.0.1",
    "@types/ttf2eot": "^2.0.0",
    "@types/ttf2woff": "^2.0.2",
    "@types/ttf2woff2": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "child_process": "^1.0.2",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.5.3",
    "eslint-plugin-jsdoc": "^39.3.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-tsdoc": "^0.2.16",
    "jest": "^28.1.1",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "semantic-release": "^19.0.3",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.8.1",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.4"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "rootDir": "./src",
    "globals": {
      "ts-jest": {
        "tsconfig": "./src/tsconfig.test.json"
      }
    }
  },
  "engines": {
    "node": ">= 12"
  }
}
